generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model parking_in {
  id         Int      @id @default(autoincrement())
  platNomor  String?  @db.VarChar(10)
  waktuMasuk DateTime
  wilayah    String?  @db.VarChar(20)
  kota_provinsi String? @db.VarChar(50)
  jenisKendaraan String? @db.VarChar(20)
  imageLink      String? @db.VarChar(256)
}


model payment {
  id            Int    @id @default(autoincrement())
  totalPrice    Int
  transactionId String @db.VarChar(100)
  status        String @db.VarChar(20)
  parking_outs   parking_out[] 
}

model parking_out {
  id          Int      @id @default(autoincrement())
  platNomor   String   @db.VarChar(10)
  waktuMasuk  DateTime
  waktuKeluar DateTime
  totalTime   Int
  wilayah    String  @db.VarChar(20)
  kota_provinsi String @db.VarChar(50)
  jenisKendaraan String @db.VarChar(20)
  imageLink      String @db.VarChar(256)
  paymentId      Int 
  payment       payment @relation(fields: [paymentId], references: [id])
}

model data_excel{
  id            Int    @id @default(autoincrement())
  fileName      String @db.VarChar(40)
  excelLink     String @db.VarChar(256)
}